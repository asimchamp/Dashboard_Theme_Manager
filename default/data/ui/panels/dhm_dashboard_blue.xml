<panel depends="$alwaysHideCSS$">
   <html>
      <style>
         /* ======================================================================
         OCEAN BREEZE THEME - PREMIUM SPLUNK DASHBOARD DESIGN
         Professional Blue/Aqua Palette with Light Mode Support
         HTML AND CSS ONLY - NO SCRIPT
         ====================================================================== */
         
         /* ----------------------------------------------------------------------
         DESIGN TOKENS
         ---------------------------------------------------------------------- */
         :root {
            --color-bg-base: #f0f4f8;
            --color-bg-elevated: #ffffff;
            --color-bg-hover: #e6eef5;
            --color-bg-active: #d9e6f2;
            --color-text-primary: #1e3a5f;
            --color-text-secondary: #4a6382;
            --color-text-muted: #6b8299;
            --color-accent-primary: #0ea5e9;
            --color-accent-secondary: #06b6d4;
            --color-border-subtle: rgba(14, 165, 233, 0.12);
            --color-border-strong: rgba(14, 165, 233, 0.25);
            --radius-xs: 4px;
            --radius-sm: 8px;
            --radius-md: 12px;
            --radius-lg: 18px;
            --radius-xl: 24px;
            --space-1: 4px;
            --space-2: 8px;
            --space-3: 12px;
            --space-4: 16px;
            --space-5: 20px;
            --space-6: 24px;
            --space-7: 32px;
            --space-8: 40px;
            --space-9: 56px;
            --space-10: 72px;
            --shadow-1: 0 1px 3px rgba(14, 165, 233, 0.12);
            --shadow-2: 0 4px 12px rgba(14, 165, 233, 0.15);
            --shadow-3: 0 12px 24px rgba(14, 165, 233, 0.18);
            --shadow-4: 0 20px 40px rgba(14, 165, 233, 0.22);
            --motion-fast: 120ms;
            --motion-medium: 280ms;
            --motion-slow: 620ms;
            --ease-standard: cubic-bezier(0.4,0.0,0.2,1);
            --ease-emphasized: cubic-bezier(0.2,0.0,0.0,1);
            /* SVG BACKGROUND (SAFE MULTI-LINE SPLIT) */
            /* Pattern: Wave Pattern */
            --blue-bg-pattern: url('data:image/svg+xml;utf8,\
%3Csvg width="100" height="40" viewBox="0 0 100 40" xmlns="http://www.w3.org/2000/svg"%3E\
%3Cpath d="M0 20 Q 25 10, 50 20 T 100 20" fill="none" stroke="%230ea5e9" stroke-width="1" stroke-opacity="0.1" /%3E\
%3Cpath d="M0 30 Q 25 20, 50 30 T 100 30" fill="none" stroke="%2306b6d4" stroke-width="1" stroke-opacity="0.05" /%3E\
%3C/svg%3E');
         }
         
         /* ----------------------------------------------------------------------
         GLOBAL BASELINE
         ---------------------------------------------------------------------- */
         body {
            background-color: var(--color-bg-base);
            color: var(--color-text-primary);
            font-smoothing: antialiased;
         }
         
         .dashboard-body {
            background-image: var(--blue-bg-pattern);
            background-size: 100px 40px;
            background-attachment: fixed;
         }
         
         .dashboard-view {
            padding-bottom: var(--space-10);
         }
         
         /* ----------------------------------------------------------------------
         DASHBOARD HEADER
         ---------------------------------------------------------------------- */
         .dashboard-header {
            position: relative;
            padding-top: var(--space-9);
            padding-bottom: var(--space-6);
            animation: headerReveal var(--motion-slow) var(--ease-emphasized) both;
         }
         
         @keyframes headerReveal {
            0% { opacity: 0; transform: translateY(-20px); }
            100% { opacity: 1; transform: translateY(0); }
         }
         
         .dashboard-title {
            font-size: 34px;
            font-weight: 700;
            letter-spacing: 0.04em;
            margin-bottom: var(--space-2);
            color: var(--color-text-primary);
         }
         
         .dashboard-description {
            font-size: 14px;
            color: var(--color-text-secondary);
            max-width: 720px;
            line-height: 1.6;
         }
         
         /* ----------------------------------------------------------------------
         THEME BADGE
         ---------------------------------------------------------------------- */
         .dashboard-header::after {
            content: "OCEAN BREEZE THEME";
            position: fixed;
            top: 125px;
            right: var(--space-6);
            padding: var(--space-2) var(--space-4);
            font-size: 11px;
            letter-spacing: 0.18em;
            text-transform: uppercase;
            background: linear-gradient(135deg, var(--color-accent-primary), var(--color-accent-secondary));
            color: #ffffff;
            border-radius: 999px;
            box-shadow: var(--shadow-2);
            pointer-events: none;
            opacity: 0;
            animation: badgeReveal var(--motion-slow) var(--ease-standard) forwards;
            animation-delay: 420ms;
            z-index: 999;
         }
         
         @keyframes badgeReveal {
            0% { opacity: 0; transform: translateY(10px) scale(0.96); }
            100% { opacity: 1; transform: translateY(0) scale(1); }
         }
         
         /* ----------------------------------------------------------------------
         DASHBOARD WATERMARK
         ---------------------------------------------------------------------- */
         .dashboard-body::after {
            content: "Splunk Theme Manager";
            position: fixed;
            bottom: 18px;
            right: 22px;
            font-size: 11px;
            letter-spacing: 0.18em;
            text-transform: uppercase;
            color: rgba(74, 99, 130, 0.35);
            pointer-events: none;
            z-index: 0;
            animation: watermarkFade 2.4s ease forwards;
         }
         
         @keyframes watermarkFade {
            from { opacity: 0; transform: translateY(6px); }
            to { opacity: 1; transform: translateY(0); }
         }
         
         /* ----------------------------------------------------------------------
         INPUTS AND FILTERS
         ---------------------------------------------------------------------- */
         .input {
            background-color: var(--color-bg-elevated);
            border-radius: var(--radius-md);
            padding: var(--space-4);
            margin-bottom: var(--space-4);
            box-shadow: var(--shadow-1);
            transition: box-shadow var(--motion-medium) var(--ease-standard),
                        transform var(--motion-medium) var(--ease-standard);
         }
         
         .input:hover {
            box-shadow: var(--shadow-2);
            transform: translateY(-2px);
         }
         
         .input label {
            font-size: 11px;
            letter-spacing: 0.12em;
            text-transform: uppercase;
            color: var(--color-text-secondary);
            margin-bottom: var(--space-2);
            display: block;
            font-weight: 600;
         }
         
         .input select,
         .input input {
            width: 100%;
            background-color: var(--color-bg-base);
            color: var(--color-text-primary);
            border: 1px solid var(--color-border-subtle);
            border-radius: var(--radius-sm);
            padding: var(--space-3);
            transition: border-color var(--motion-fast) var(--ease-standard),
                        box-shadow var(--motion-fast) var(--ease-standard);
         }
         
         .input select:focus,
         .input input:focus {
            outline: none;
            border-color: var(--color-accent-primary);
            box-shadow: 0 0 0 3px rgba(14, 165, 233, 0.15);
         }
         
         /* ----------------------------------------------------------------------
         PANEL CONTAINERS
         ---------------------------------------------------------------------- */
         .dashboard-panel {
            background-color: var(--color-bg-elevated);
            border-radius: var(--radius-lg);
            padding: var(--space-6);
            margin-bottom: var(--space-6);
            box-shadow: var(--shadow-1);
            border: 1px solid var(--color-border-subtle);
            transition: transform var(--motion-medium) var(--ease-standard),
                        box-shadow var(--motion-medium) var(--ease-standard),
                        border-color var(--motion-medium) var(--ease-standard);
            animation: panelEntry var(--motion-slow) var(--ease-standard) both;
         }
         
         @keyframes panelEntry {
            0% { opacity: 0; transform: translateY(24px); }
            100% { opacity: 1; transform: translateY(0); }
         }
         
         .dashboard-panel:hover {
            transform: translateY(-4px);
            box-shadow: var(--shadow-3);
            border-color: var(--color-border-strong);
         }
         
         .dashboard-panel .panel-head {
            border-bottom: 1px solid var(--color-border-subtle);
            padding-bottom: var(--space-3);
            margin-bottom: var(--space-4);
         }
         
         .dashboard-panel .panel-title {
            font-size: 13px;
            font-weight: 600;
            letter-spacing: 0.08em;
            text-transform: uppercase;
            color: var(--color-text-secondary);
         }
         
         /* ----------------------------------------------------------------------
         KPI AND SINGLE VALUE PANELS
         ---------------------------------------------------------------------- */
         .single-value {
            font-size: 42px;
            font-weight: 700;
            letter-spacing: 0.04em;
            color: var(--color-accent-primary);
            animation: kpiPop var(--motion-medium) var(--ease-emphasized) both;
         }
         
         @keyframes kpiPop {
            0% { opacity: 0; transform: scale(0.9); }
            100% { opacity: 1; transform: scale(1); }
         }
         
         .single-value-title {
            font-size: 12px;
            text-transform: uppercase;
            letter-spacing: 0.16em;
            color: var(--color-text-secondary);
            margin-top: var(--space-2);
            font-weight: 600;
         }
         
         .single-value::after {
            content: "";
            display: block;
            height: 3px;
            width: 48px;
            margin-top: var(--space-3);
            background: linear-gradient(90deg, var(--color-accent-primary), var(--color-accent-secondary));
            border-radius: 999px;
            animation: underlineGrow var(--motion-slow) var(--ease-standard) both;
         }
         
         @keyframes underlineGrow {
            0% { width: 0; }
            100% { width: 48px; }
         }
         
         /* ----------------------------------------------------------------------
         CHARTS AND GRAPHS
         ---------------------------------------------------------------------- */
         .chart svg {
            animation: chartReveal var(--motion-slow) var(--ease-emphasized) both;
            animation-delay: 180ms;
         }
         
         @keyframes chartReveal {
            0% { opacity: 0; transform: scale(0.95); }
            100% { opacity: 1; transform: scale(1); }
         }
         
         /* Bar charts */
         .highcharts-series rect {
            rx: 4;
            ry: 4;
            transition: fill var(--motion-fast) var(--ease-standard),
                        transform var(--motion-fast) var(--ease-standard);
         }
         
         .highcharts-series rect:hover {
            transform: scale(1.05);
            filter: brightness(1.1);
         }
         
         /* Line charts */
         .highcharts-graph {
            stroke-width: 2.5;
            stroke-linecap: round;
            stroke-linejoin: round;
         }
         
         .highcharts-area {
            fill-opacity: 0.25;
         }
         
         /* Axis styling */
         .highcharts-axis-labels text,
         .highcharts-legend-item text {
            fill: var(--color-text-secondary);
            font-size: 11px;
         }
         
         .highcharts-axis-line,
         .highcharts-grid-line {
            stroke: var(--color-border-subtle);
         }
         
         /* ----------------------------------------------------------------------
         TABLES
         ---------------------------------------------------------------------- */
         table {
            width: 100%;
            border-collapse: separate;
            border-spacing: 0 var(--space-2);
         }
         
         thead th {
            font-size: 11px;
            letter-spacing: 0.14em;
            text-transform: uppercase;
            color: var(--color-text-secondary);
            padding: var(--space-3);
            text-align: left;
            font-weight: 600;
            background-color: var(--color-bg-base);
         }
         
         tbody tr {
            background-color: var(--color-bg-elevated);
            border-radius: var(--radius-sm);
            box-shadow: var(--shadow-1);
            transition: transform var(--motion-fast) var(--ease-standard),
                        box-shadow var(--motion-fast) var(--ease-standard),
                        background-color var(--motion-fast) var(--ease-standard);
         }
         
         tbody tr:hover {
            transform: scale(1.01);
            box-shadow: var(--shadow-2);
            background-color: var(--color-bg-hover);
         }
         
         tbody td {
            padding: var(--space-3) var(--space-4);
            font-size: 13px;
            color: var(--color-text-primary);
            border-top: 1px solid var(--color-border-subtle);
         }
         
         tbody td:first-child {
            border-top-left-radius: var(--radius-sm);
            border-bottom-left-radius: var(--radius-sm);
         }
         
         tbody td:last-child {
            border-top-right-radius: var(--radius-sm);
            border-bottom-right-radius: var(--radius-sm);
         }
         
         /* ----------------------------------------------------------------------
         LEGENDS AND TOOLTIPS
         ---------------------------------------------------------------------- */
         .legend {
            display: flex;
            gap: var(--space-4);
            margin-top: var(--space-4);
            flex-wrap: wrap;
         }
         
         .legend-item {
            font-size: 12px;
            color: var(--color-text-secondary);
            transition: color var(--motion-fast) var(--ease-standard),
                        transform var(--motion-fast) var(--ease-standard);
         }
         
         .legend-item:hover {
            color: var(--color-text-primary);
            transform: translateY(-2px);
         }
         
         .highcharts-tooltip {
            filter: drop-shadow(0 8px 16px rgba(14, 165, 233, 0.25));
         }
         
         .highcharts-tooltip-box {
            fill: var(--color-bg-elevated);
            stroke: var(--color-border-strong);
         }
         
         /* ----------------------------------------------------------------------
         SCROLLBAR
         ---------------------------------------------------------------------- */
         ::-webkit-scrollbar {
            width: 10px;
            height: 10px;
         }
         
         ::-webkit-scrollbar-track {
            background-color: var(--color-bg-base);
         }
         
         ::-webkit-scrollbar-thumb {
            background-color: var(--color-accent-primary);
            border-radius: 999px;
            opacity: 0.6;
         }
         
         ::-webkit-scrollbar-thumb:hover {
            opacity: 1;
         }
         
         /* ----------------------------------------------------------------------
         MICRO-INTERACTIONS AND DETAILS
         ---------------------------------------------------------------------- */
         .dashboard-panel::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 3px;
            background: linear-gradient(90deg, var(--color-accent-primary), var(--color-accent-secondary));
            border-radius: var(--radius-lg) var(--radius-lg) 0 0;
            opacity: 0;
            transition: opacity var(--motion-medium) var(--ease-standard);
         }
         
         .dashboard-panel:hover::before {
            opacity: 1;
         }
         
         /* Button styling */
         .btn,
         button {
            background-color: var(--color-accent-primary);
            color: #ffffff;
            border: none;
            border-radius: var(--radius-sm);
            padding: var(--space-2) var(--space-4);
            font-size: 13px;
            font-weight: 600;
            letter-spacing: 0.04em;
            cursor: pointer;
            transition: all var(--motion-fast) var(--ease-standard);
         }
         
         .btn:hover,
         button:hover {
            background-color: var(--color-accent-secondary);
            transform: translateY(-1px);
            box-shadow: var(--shadow-2);
         }
         
         /* ----------------------------------------------------------------------
         STATE-BASED DEPTH UTILITIES
         ---------------------------------------------------------------------- */
         .u-depth-1 { box-shadow: var(--shadow-1); }
         .u-depth-2 { box-shadow: var(--shadow-2); }
         .u-depth-3 { box-shadow: var(--shadow-3); }
         .u-depth-4 { box-shadow: var(--shadow-4); }
         
         .u-radius-xs { border-radius: var(--radius-xs); }
         .u-radius-sm { border-radius: var(--radius-sm); }
         .u-radius-md { border-radius: var(--radius-md); }
         .u-radius-lg { border-radius: var(--radius-lg); }
         .u-radius-xl { border-radius: var(--radius-xl); }
         
         /* ----------------------------------------------------------------------
         ENHANCED PANEL ROW LAYOUT
         ---------------------------------------------------------------------- */
         .dashboard-row {
            display: flex;
            gap: var(--space-4);
            margin-bottom: var(--space-4);
         }
         
         .dashboard-row .dashboard-panel {
            flex: 1;
            min-width: 0;
         }
         
         /* ----------------------------------------------------------------------
         FOCUS AND ACTIVE STATES
         ---------------------------------------------------------------------- */
         .input select:focus,
         .input input:focus,
         .btn:focus,
         button:focus {
            outline: none;
            box-shadow: 0 0 0 3px rgba(14, 165, 233, 0.25);
         }
         
         .btn:active,
         button:active {
            transform: scale(0.98);
         }
         
         /* ----------------------------------------------------------------------
         STAGGERED PANEL ANIMATION
         ---------------------------------------------------------------------- */
         .dashboard-panel:nth-child(1) {
            animation-delay: 0ms;
         }
         
         .dashboard-panel:nth-child(2) {
            animation-delay: 80ms;
         }
         
         .dashboard-panel:nth-child(3) {
            animation-delay: 160ms;
         }
         
         .dashboard-panel:nth-child(4) {
            animation-delay: 240ms;
         }
         
         .dashboard-panel:nth-child(5) {
            animation-delay: 320ms;
         }
         
         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: SCROLLBARS (OCEANIC)
         ---------------------------------------------------------------------- */
         ::-webkit-scrollbar {
            width: 12px;
            height: 12px;
            background-color: #f0f9ff;
         }
         
         ::-webkit-scrollbar-track {
            background: #e0f2fe;
            border-radius: 6px;
         }
         
         ::-webkit-scrollbar-thumb {
            background: #7dd3fc;
            border: 3px solid #e0f2fe;
            border-radius: 6px;
         }
         
         ::-webkit-scrollbar-thumb:hover {
            background: #0ea5e9;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: INPUTS AND FORM ELEMENTS (LIQUID)
         ---------------------------------------------------------------------- */
         .splunk-text-input input,
         .splunk-dropdown .select2-choice,
         .splunk-timerange-picker .btn {
            background: #fff !important;
            border: 1px solid #bae6fd !important;
            color: #0c4a6e !important;
            border-radius: 12px !important;
            box-shadow: 0 4px 6px -1px rgba(14, 165, 233, 0.1) !important;
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1) !important;
            height: 36px !important;
            line-height: 34px !important;
         }

         .splunk-text-input input:focus,
         .splunk-dropdown .select2-choice:focus {
            border-color: #0ea5e9 !important;
            box-shadow: 0 0 0 3px rgba(14, 165, 233, 0.2) !important;
            transform: translateY(-1px);
         }

         /* Dropdowns */
         .select2-drop, .dropdown-menu {
            background: #fff !important;
            border: 1px solid #e0f2fe !important;
            box-shadow: 0 10px 30px rgba(14, 165, 233, 0.15) !important;
            border-radius: 12px !important;
            margin-top: 8px !important;
         }
         
         .select2-results .select2-result-label {
             color: #0c4a6e !important;
             padding: 10px 14px !important;
             border-radius: 8px !important;
             margin: 2px !important;
         }

         .select2-results .select2-highlighted {
            background: #f0f9ff !important;
            color: #0ea5e9 !important;
            font-weight: 600;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: BUTTONS (BUBBLE)
         ---------------------------------------------------------------------- */
         .btn-primary {
            background: linear-gradient(135deg, #0ea5e9, #06b6d4) !important;
            border: none !important;
            color: #fff !important;
            border-radius: 25px !important;
            box-shadow: 0 4px 15px rgba(6, 182, 212, 0.3) !important;
            font-weight: 600 !important;
            padding: 6px 20px !important;
            transition: all 0.3s ease !important;
         }

         .btn-primary:hover {
            transform: translateY(-2px);
            box-shadow: 0 8px 25px rgba(6, 182, 212, 0.4) !important;
         }
         
         .btn-default {
             background: #fff !important;
             border: 1px solid #e0f2fe !important;
             color: #0c4a6e !important;
             border-radius: 25px !important;
             box-shadow: 0 2px 5px rgba(0,0,0,0.05) !important;
         }
         
         .btn-default:hover {
             background: #f0f9ff !important;
             border-color: #bae6fd !important;
             color: #0284c7 !important;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: TABLES (SOFT ROW)
         ---------------------------------------------------------------------- */
         .table {
            background: #fff !important;
            border: none !important;
            border-radius: 16px !important;
            overflow: hidden !important;
            box-shadow: 0 10px 30px rgba(14, 165, 233, 0.05) !important;
         }

         .table th {
            background: #f0f9ff !important;
            color: #0284c7 !important;
            border-bottom: 1px solid #e0f2fe !important;
            font-weight: 600 !important;
            text-transform: uppercase !important;
            font-size: 11px !important;
            letter-spacing: 0.5px !important;
            padding: 15px !important;
         }

         .table td {
            border-top: 1px solid #f0f9ff !important;
            color: #334155 !important;
            padding: 12px 15px !important;
            transition: all 0.2s ease;
         }
         
         .table tbody tr:hover td {
            background: #f0f9ff !important;
            color: #0284c7 !important;
            transform: scale(1.005); /* Subtle pop */
         }
         
         .table td.numeric {
            font-weight: 600 !important;
            color: #0891b2 !important;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: MODALS (AIRY)
         ---------------------------------------------------------------------- */
         .modal-content {
            background: #fff !important;
            border: none !important;
            box-shadow: 0 25px 80px rgba(14, 165, 233, 0.25) !important;
            border-radius: 24px !important;
         }
         
         .modal-header {
             border-bottom: 1px solid #f0f9ff !important;
             background: transparent !important;
         }
         
         .modal-footer {
             border-top: 1px solid #f0f9ff !important;
             background: #f9fafb !important;
             border-radius: 0 0 24px 24px !important;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: PROGRESS BARS (GRADIENT)
         ---------------------------------------------------------------------- */
         .progress-bar {
             background: linear-gradient(90deg, #0ea5e9, #06b6d4, #22d3ee) !important;
             border-radius: 10px !important;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: HIGHCHARTS
         ---------------------------------------------------------------------- */
         .highcharts-grid-line {
             stroke: #e0f2fe !important;
         }
         
         .highcharts-axis-line {
             stroke: #bae6fd !important;
         }
         
         .highcharts-area {
             fill-opacity: 0.1 !important;
         }

         /* ----------------------------------------------------------------------
         REMASTERED COMPONENT: RESPONSIVE AND MOBILE (OCEAN)
         ---------------------------------------------------------------------- */
         @media screen and (max-width: 768px) {
             .dashboard-panel {
                 border-radius: 12px !important;
                 margin-bottom: 15px !important;
                 box-shadow: 0 4px 10px rgba(0,0,0,0.05) !important;
             }
             
             /* Liquid stack */
             .splunk-text-input, 
             .splunk-dropdown,
             .splunk-timerange-picker {
                 width: 100% !important;
                 display: flex !important;
                 flex-direction: column;
                 margin-bottom: 12px;
             }
             
             /* Bigger touch targets */
             .btn {
                 padding: 12px 20px !important;
                 font-size: 16px !important;
             }
             
             /* Simplified background */
             .dashboard-body {
                 background-attachment: scroll !important;
             }
             
             /* Scrollable tables */
             .panel-element-row {
                 overflow-x: auto;
             }
             
             .table {
                 min-width: 600px;
             }
             
             /* Adjust headings */
             h2, h3 {
                 color: #0284c7 !important;
                 text-align: center;
             }
             
             /* Centered single values */
             .single-value {
                 text-align: center;
             }
             
             .single-result {
                 font-size: 32px !important;
             }
         }
         
         /* Print Optimization */
         @media print {
             .dashboard-body, body {
                 background: #fff !important;
                 color: #000 !important;
             }
             
             .dashboard-panel {
                 box-shadow: none !important;
                 border: 1px solid #ddd !important;
             }
             
             /* Ensure text is black for printing */
             p, h1, h2, h3, h4, th, td, li {
                 color: #000 !important;
             }
             
             .highcharts-container {
                 filter: grayscale(100%);
             }
         }
         
